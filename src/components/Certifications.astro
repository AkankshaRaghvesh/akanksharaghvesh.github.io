---

---

<section class="certifications-section" id="certifications">
    <div class="container">
        <div class="section-header" data-aos="fade-up">
            <h2 class="section-title">Certifications</h2>
            <div class="section-line"></div>
        </div>
        <div class="certifications-grid">
            <div
                class="certification-card"
                data-aos="fade-up"
                data-aos-delay="100"
            >
                <div class="certification-content">
                    <div class="cert-logo">
                        <img
                            src="/assets/img/Comp.png"
                            alt="CompTIA Logo"
                            loading="lazy"
                        />
                        <div class="verified-badge">
                            <svg viewBox="0 0 24 24" class="verified-icon">
                                <path
                                    d="M9 16.17L4.83 12l-1.42 1.41L9 19 21 7l-1.41-1.41z"
                                ></path>
                            </svg>
                        </div>
                    </div>
                    <div class="cert-details">
                        <h4 class="cert-name">CompTIA Security+</h4>
                        <p class="issuer">CompTIA</p>
                        <p class="candidate-id">
                            Candidate ID - COMP001022360505
                        </p>
                        <a
                            href="https://www.credly.com/badges/84bbd1ee-9f65-4bd2-841e-e15c99479001/linked_in_profile"
                            class="cert-link"
                            target="_blank">View Certificate</a
                        >
                    </div>
                </div>
            </div>

            <div
                class="certification-card"
                data-aos="fade-up"
                data-aos-delay="125"
            >
                <div class="certification-content">
                    <div class="cert-logo">
                        <img
                            src="/assets/img/ITS.png"
                            alt="Pearson Logo"
                            loading="lazy"
                        />
                        <div class="verified-badge">
                            <svg viewBox="0 0 24 24" class="verified-icon">
                                <path
                                    d="M9 16.17L4.83 12l-1.42 1.41L9 19 21 7l-1.41-1.41z"
                                ></path>
                            </svg>
                        </div>
                    </div>
                    <div class="cert-details">
                        <h4 class="cert-name">IT Specialist</h4>
                        <p class="issuer">Pearson (IT Support)</p>
                        <a
                            href="https://www.credly.com/badges/7ba48075-e1eb-421b-83e0-610762e7a5dc/public_url"
                            class="cert-link"
                            target="_blank">View Certificate</a
                        >
                    </div>
                </div>
            </div>

            <div
                class="certification-card"
                data-aos="fade-up"
                data-aos-delay="150"
            >
                <div class="certification-content">
                    <div class="cert-logo">
                        <img
                            src="/assets/img/cisco.png"
                            alt="Cisco Logo"
                            loading="lazy"
                        />
                        <div class="verified-badge">
                            <svg viewBox="0 0 24 24" class="verified-icon">
                                <path
                                    d="M9 16.17L4.83 12l-1.42 1.41L9 19 21 7l-1.41-1.41z"
                                ></path>
                            </svg>
                        </div>
                    </div>
                    <div class="cert-details">
                        <h4 class="cert-name">
                            Cisco Certified Support Technician
                        </h4>
                        <p class="issuer">Cisco</p>
                        <a
                            href="https://www.credly.com/badges/605bc1c3-3ce1-487f-90c1-286debe4ad07/public_url"
                            class="cert-link"
                            target="_blank">View Certificate</a
                        >
                    </div>
                </div>
            </div>

            <div
                class="certification-card"
                data-aos="fade-up"
                data-aos-delay="200"
            >
                <div class="certification-content">
                    <div class="cert-logo">
                        <img src="/assets/img/USDH.png" alt="ICS Logo" />
                        <div class="verified-badge">
                            <svg viewBox="0 0 24 24" class="verified-icon">
                                <path
                                    d="M9 16.17L4.83 12l-1.42 1.41L9 19 21 7l-1.41-1.41z"
                                ></path>
                            </svg>
                        </div>
                    </div>
                    <div class="cert-details">
                        <h4 class="cert-name">
                            210W-06 ICS Cybersecurity Threats Certification
                        </h4>
                        <p class="issuer">ICS</p>
                        <a
                            href="https://drive.google.com/file/d/1LVveozZUK8KU113fDDRvGnPr9DLcjIn5/view?usp=share_link"
                            class="cert-link"
                            target="_blank">View Certificate</a
                        >
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
